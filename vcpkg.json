{
  "name": "heos2mqtt",
  "version-string": "0.1.0",
  "description": "Service that bridges HEOS CLI output to MQTT.",
  "builtin-baseline": "84bab45d415d22042bd0b9081aea57f362da3f35",
  "dependencies": [
    {
      "name": "boost-asio",
      "version>=": "1.90.0"
    },
    {
      "name": "boost-json",
      "version>=": "1.90.0"
    },
    {
      "name": "boost-mqtt5",
      "version>=": "1.90.0"
    },
    {
      "name": "boost-beast",
      "version>=": "1.90.0"
    },
    {
      "name": "fmt",
      "version>=": "12.1.0"
    },
    {
      "name": "catch2",
      "version>=": "3.11.0"
    }
  ],
  "features": {
    "stacktrace-backtrace": {
      "description": "Use boost-stacktrace with libbacktrace support.",
      "dependencies": [
        {
          "name": "boost-stacktrace",
          "features": [
            "backtrace"
          ],
          "version>=": "1.90.0"
        }
      ]
    },
    "stacktrace-basic": {
      "description": "Use boost-stacktrace with basic support.",
      "dependencies": [
        {
          "name": "boost-stacktrace",
          "default-features": false,
          "version>=": "1.90.0"
        }
      ]
    }
  },
  "default-features": [
    "stacktrace-backtrace"
  ]
}